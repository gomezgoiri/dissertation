@inproceedings{mayer_semantic_2013,
	address = {New York, {NY}, {USA}},
	series = {{UbiComp} '13 Adjunct},
	title = {Semantic metadata to support device interaction in smart environments},
	isbn = {978-1-4503-2215-7},
	url = {http://doi.acm.org/10.1145/2494091.2497584},
	doi = {10.1145/2494091.2497584},
	abstract = {Facilitating the interaction of human users and machines with smart devices is important to drive the successful adoption of the Internet of Things in people's homes and at their workplaces. In this paper, we present a system that helps users control their smart environment, by embedding semantic metadata in the representations of smart things. The system enables users to specify a desirable state of their smart environment and produces a machine-readable description that details which steps are necessary to reach this state, where each step corresponds to a Web request to a smart device. A client application that, for instance, runs on the user's smartphone, then implements these steps to reconfigure the user's smart environment. We report on our experiences when integrating semantic technologies with smart devices and on two use cases from the home and office automation domains that we implemented in our office space.},
	urldate = {2013-10-28},
	booktitle = {Proceedings of the 2013 {ACM} conference on Pervasive and ubiquitous computing adjunct publication},
	publisher = {{ACM}},
	author = {Mayer, Simon and Basler, Gianin},
	year = {2013},
	keywords = {machine-machine interaction, reasoning, restdesc, Semantics, smart environments, Web of Things},
	pages = {1505–1514}
},