	keywords = {Computer Applications in Earth Sciences, Geographical Information {Systems/Cartography}, Information Systems Applications ({incl.Internet)}, Internet of Things, {IoT}, Personal Computing, Remote {Sensing/Photogrammetry}, Sensor, Special Purpose and Application-Based Systems, ssn},
	pages = {72--86}
},


